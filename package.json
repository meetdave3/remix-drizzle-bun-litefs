{
  "name": "bun-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "postbuild": "sed -i.bak 's/\"react-dom\\/server\"/\"react-dom\\/server.node\"/' build/server/index.js && rm -f build/server/index.js.bak",
    "generate": "drizzle-kit generate --schema=app/db/schema.ts --dialect=sqlite",
    "dev": "bunx --bun vite",
    "start": "bun run server.ts",
    "migrate": "bun run migrate.ts",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "@react-router/fs-routes": "^7.0.2",
    "@react-router/node": "^7.0.2",
    "@react-router/serve": "^7.0.2",
    "drizzle-orm": "^0.38.2",
    "hono": "^4.1.0",
    "isbot": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.17.0",
    "@react-router/dev": "^7.0.2",
    "@types/bun": "^1.1.14",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.30.1",
    "better-sqlite3": "^11.7.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.6",
    "vite": "^6.0.2",
    "vite-tsconfig-paths": "^4.2.1"
  }
}